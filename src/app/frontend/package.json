{
  "name": "vuege-frontend",
  "version": "0.1.0",
  "description": "Frontend для системы учета организационных единиц Vuege",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:quasar": "quasar dev",
    "dev:mobile": "quasar dev --mode mobile",
    "build": "vite build",
    "build:quasar": "quasar build",
    "preview": "vite preview",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@apollo/client": "^3.8.8",
    "@quasar/extras": "^1.16.9",
    "@types/leaflet": "^1.9.8",
