    "graphql": "^16.8.1",
    "leaflet": "^1.9.4",
    "quasar": "^2.18.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@quasar/vite-plugin": "^1.8.0",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.20.1",
    "prettier": "^3.2.5",
    "sass-embedded": "^1.90.0",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
